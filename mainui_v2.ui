                <property name="styleSheet">
                 <string notr="true">QTextEdit {
	
	font: 57 8pt &quot;Dubai Medium&quot;;
    background-color: rgb(255, 255, 255); /* Màu nền của QTextEdit */
    border-radius: 7px;
    border: 2px solid rgb(0, 80, 118); /* Đường viền */
    color: rgb(0, 0, 0); /* Màu chữ */
    selection-background-color: rgb(0, 128, 255); /* Màu nền khi chọn văn bản */
    selection-color: rgb(255, 255, 255); /* Màu chữ khi chọn văn bản */
}

/* Tùy chỉnh thanh cuộn khi QTextEdit hiển thị thanh cuộn */
QTextEdit QScrollBar:vertical {
    background-color: rgb(240, 240, 240); /* Màu nền thanh cuộn */
    width: 12px; /* Độ rộng của thanh cuộn */
    margin: 15px 0 15px 0;
    border-radius: 6px; /* Bo góc cho thanh cuộn */
}

QTextEdit QScrollBar::handle:vertical {
    background-color: rgb(0, 80, 118); /* Màu thanh cuộn */
    min-height: 20px;
    border-radius: 6px; /* Bo góc cho thanh cuộn */
}

QTextEdit QScrollBar::handle:vertical:hover {
    background-color: rgb(0, 128, 255); /* Màu thanh cuộn khi hover */
}

QTextEdit QScrollBar::add-line:vertical, QTextEdit QScrollBar::sub-line:vertical {
    border: none;
    background-color: none;
    height: 0px;
}

QTextEdit QScrollBar::add-page:vertical, QTextEdit QScrollBar::sub-page:vertical {
    background: none;
}

/* Tương tự cho thanh cuộn ngang nếu cần */
QTextEdit QScrollBar:horizontal {
    background-color: rgb(240, 240, 240); /* Màu nền thanh cuộn */
