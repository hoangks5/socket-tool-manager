QPushButton {
    border-radius: 7px;
    border: 2px solid #c2185b; /* Màu viền đỏ hồng đậm */
    background: linear-gradient(145deg, #ff4081, #c2185b); /* Gradient từ hồng nhạt đến đỏ đậm */
    color: rgb(0, 0, 0);
    height: 20px;
    text-align: center; /* Sửa chính tả từ text-algin thành text-align */
    font-weight: bold; /* Chữ đậm để tạo ấn tượng */
    box-shadow: 4px 4px 8px rgba(0, 0, 0, 0.3); /* Đổ bóng cho nút bấm */
}

QPushButton:hover {
    border: 2px solid #f50057; /* Màu viền hồng sáng hơn khi hover */
    background:#4A569D; /* Gradient sáng hơn khi hover */
    color: #ffffff; /* Đảm bảo màu chữ vẫn trắng khi hover */
    box-shadow: 4px 4px 12px rgba(0, 0, 0, 0.4); /* Đổ bóng nổi bật hơn khi hover */
}

QPushButton:pressed {
    background: linear-gradient(145deg, #d81b60, #c2185b); /* Gradient tối hơn khi nhấn */
    border: 2px solid #b71c1c; /* Màu viền đỏ đậm hơn khi nhấn */
    box-shadow: inset 4px 4px 8px rgba(0, 0, 0, 0.5); /* Hiệu ứng đổ bóng lõm khi nhấn */
}


QGroupBox {
    border: 2px solid #666;
    border-radius: 10px;
    padding: 10px;
    color: white;
    font-weight: bold;
}

QGroupBox:title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    padding: 0 10px;
    background: transparent;
    color: rgb(0, 0, 0);
    font-size: 16px;
}
QLineEdit {
